    "import torch\n",
    "\n",
    "if torch.backends.mps.is_available():\n",
    "    mps_device = torch.device(\"mps\")\n",
    "    x = torch.ones(1, device=mps_device)\n",
    "    print (x)\n",
    "    print(\"GPU is available and MPS is enabled.\")\n",
    "else:\n",
    "    print(\"GPU is not available or MPS is not enabled.\")"
